{
  "name": "ds/craft-slider",
  "description": "A plugin for Craft CMS that helps you add testimonial slider in your website.",
  "type": "craft-plugin",
  "version": "1.0.0",
  "keywords": ["craft",
  	"cms",
  	"craftcms",
      	"craft-plugin",
        "slider",
      	"testimonial-slider"],
  "license": "MIT",
  "authors": [
  	{
          "name": "Dotsquares",
          "homepage": "https://dotsquares.com/"
        }
  ],
  "support": {
    "email": "support.extensions@dotsquares.com",
    "issues": "https://github.com/dotsquares-support/Testimonial-Slider/issues",
    "source": "https://github.com/dotsquares-support/Testimonial-Slider/",
    "docs": "https://github.com/dotsquares-support/Testimonial-Slider/blob/master/README.md"
  },
  "require": {
    "craftcms/cms": "^3.1.0"
  },
  "autoload": {
    "psr-4": {
      "ds\\slider\\": "src/"
    }
  },
  "extra": {
    "name": "Testimonial Slider",
    "handle": "slider",
    "description": "Add testimonial Slider",
    "hasCpSection": false,
    "hasSettings": true,
    "class": "ds\\slider\\Slider"
  }
}
